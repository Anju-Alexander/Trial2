{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
   "packageRules": [
    {
      "enabled": true,
      "matchPackagePatterns": ["*"],
     
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["Security"],
    "ignoreTests": true,
    "automerge": true,
    "automergeType": "pr"
  }
 
}
